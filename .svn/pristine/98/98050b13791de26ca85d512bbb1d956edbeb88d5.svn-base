import React, {useState} from 'react'

import MainPage from './pages/mainPage/mainPage';
import Login from './pages/login';

function App() {
  const [login, setLogin] = useState(true)

  const loginHandler = () => {
    setLogin(false)
  }

  return login ? 
    <Login loginHandler={loginHandler}/> : 
    <MainPage />
}

export default App;
