                    if(userSelectedBenefitsByCatalogsType[k].deliveryDate == productsSelected[i].deliveryDate){
                        tempBenefitsOrder = userSelectedBenefitsByCatalogsType[k];
                        userSelectedBenefitsByCatalogsTypeINDEX = k;
