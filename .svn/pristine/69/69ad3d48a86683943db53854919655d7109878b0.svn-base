        }
    }else{
        var cashAmount = 0;
        for(var n = 0; n < cashFlow.length; n++){
            if(cashFlow[n].benefitType == "efectivo"){
                if(cashFlow[n].amountToDiscount != undefined){
                    cashAmount = cashFlow[n].amountToDiscount;
                }
            }
