import { baseUrl, userId, token } from "./constants";
import axios from 'axios'; 

const headers = {
    'Content-Type': 'application/json',
    'userid': userId,
    'token': token
    }

export const getHttpResponse = async (url) => {
    const response = await axios.get(baseUrl + url, {headers});
    return response
}

export const postHttpResponse = async (url, body) => {
    const response = await axios.post(baseUrl + url, body, {headers});
    return response
}

